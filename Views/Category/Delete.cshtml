@model Category
<form method="post">
    <div class="mb-3">
        <label asp-for="Name" class="form-label"></label>
        <input asp-for="Name" disabled type="text" class="form-control" id="Name">
    </div>
    <div class="mb-3">
        <label asp-for="DisplayOrder" class="form-label"></label>
        <input asp-for="DisplayOrder" disabled type="text" class="form-control" id="DisplayOrder">
    </div>
    <div class="row">
        <div class="col-6 col-md-3 row">
            <button type="submit" class="btn btn-danger form-control">Delete</button>
        </div>
        <div class="col-6 col-md-3 row">
            <a asp-controller="Category" asp-action="Index" class="btn btn-outline-secondary form-control">Back To List</a>
        </div>
    </div>
</form>
